{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.ts",
        "use": "@vercel/vite",
        "config": {
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }